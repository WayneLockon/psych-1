@article{maccallum:01,
	Abstract = {This article examines effects of sample size and other design features on correspondence between factors obtained from analysis of sample data and those present in the population from which the samples were drawn. The authors extend earlier work on this question by examining these phenomena in the situation in which the common factor model does not hold exactly in the population. They present a theoretical framework for representing such lack of fit and examine its implications in the population and sample. Based on this approach it is hypothesized that lack of fit of the model in the population will not, on the average, influence recovery of population factors in analysis of sample data, regardless of degree of model error and regardless of sample size. Rather, such recovery will be affected only by phenomena related to sampling error which have been studied previously. These hypotheses are investigated and verified in 2 sampling studies, 1 using artificial data and 1 using empirical data of 55,085 Ss aged 15 yrs and older. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {MacCallum, Robert C. and Widaman, Keith F. and Preacher, Kristopher J. and Hong, Sehee},
	Date-Added = {2008-06-16 21:15:09 -0500},
	Date-Modified = {2008-06-16 21:15:35 -0500},
