@article{multicultural:pq,
	Abstract = {Considered the reliability and validity of the 78-item revised version of the Multicultural Personality Questionnaire, a multidimensional instrument aimed at measuring multicultural effectiveness among 210 students (aged 17-57 yrs). Exploratory and confirmatory factor analyses on self- and other ratings for an original set of 138 items revealed 5 reliable factors underlying the questionnaire: Cultural Empathy, Openmindedness, Emotional Stability, Social Initiative, and Flexibility. In addition, initial support for the validity of the instrument was obtained by showing convergence between self and other ratings on the scales. Moreover, the instrument was clearly able to discriminate between students with and without explicit international inspirations. The dimension of Cultural Empathy was least well supported. (PsycINFO Database Record (c) 2008 APA, all rights reserved).},
	Author = {Van der Zee, Karen I. and Van Oudenhoven, Jan Pieter},
