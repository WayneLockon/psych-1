	Title = {A psychological taxonomy of trait-descriptive terms: The interpersonal domain},
	Volume = {37},
	Year = {1979}}

@article{wilcox:87,
	Abstract = {Reviews differences between the uses of analysis of variance (ANOVA) as described in textbooks and as described in current literature to illustrate why the discrepancies should be of concern to psychologists. The author concludes that all of the conventional hypothesis testing procedures that assume equal variances should be abandoned. At a minimum, approximate solutions for handling unequal variances should be used, and wherever possible, exact 2-stage procedures should be employed. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
