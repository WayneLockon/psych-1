@article{steiger:80,
	Abstract = {Two tests for interaction effects in 2-2 proportion tables were compared theoretically and empirically. One method was based on the traditional use of the normal approximation to the binomial distribution, whereas the 2nd, suggested by E. J. Langer and R. P. Abelson (see record 1973-04425-001), used the "arc sine transform." Method 1, as suspected, performed most poorly under conditions (small N, large or small p) in which the binomial departed markedly from the normal. However, the statistic performed better than generally expected and could be used safely with small to moderate samples. Method 2 was found to be seriously biased, essentially because a nonlinear transform was applied to a linear hypothesis. Moreover, even when conditions were optimal for its use, Method 2 failed to outperform the more traditional test. It is argued that the use of Method 2 should be discontinued. (3 ref) (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Steiger, James H.},
	Journal = {Psychological Bulletin},
	Keywords = {tests for interaction effects in tables of proportions Statistics & Mathematics [2240]},
	Number = {3},
	Pages = {772-775},
	Title = {Comparison of two methods for testing linear hypotheses in tables of proportions},
	Volume = {88},
	Year = {1980}}

@article{steiger:80a,
	Abstract = {Proposes several alternative statistics for assessing the goodness-of-fit of correlational pattern hypotheses, based on the use of the Fisher r-to-z transform. Their performance, and that of traditional statistics such as likelihood ratio, are assessed in a Monte Carlo experiment. The new statistics are shown to have Type I error rate performance at smaller sample sizes, which is notably superior to their more traditional counterparts. (17 ref) (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Steiger, James H.},
	Date-Modified = {2008-06-22 15:04:22 -0500},
	Journal = {Multivariate Behavioral Research},
	Keywords = {Monte Carlo simulations of alternative statistics, assessment of goodness-of-fit of correlational pattern hypotheses Statistics & Mathematics [2240]},
	Number = {3},
	Pages = {335-352},
	Title = {Testing pattern hypotheses on correlation matrices: Alternative statistics and some empirical results},
	Volume = {15},
	Year = {1980}}

@article{stelmack:97,
	Abstract = {The relation of taxonomy and theory in the development of a paradigmatic, scientific knowledge of personality is discussed. Successful taxonomies in chemistry, biology and psychology are briefly cited to illustrate the relation of taxonomy (description) and scientific theory (explanation). From this perspective, it is argued that a descriptive typology can develop without clear knowledge of the causal relation of things, but the proof of the validity of the structure does rest on that theoretical basis, as Eysenck claimed. The greater sensitivity of introverts than extraverts to punctate, physical stimulation is promoted as an effect that may be featured in the explanation of individual differences in the extraversion trait},
	Author = {Stelmack, Robert M.},
