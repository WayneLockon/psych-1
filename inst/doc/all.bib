@article{matthews:gilliland:99,
	Abstract = {H. J. Eysenck and J. A. Gray have proposed theories of the biological bases of personality traits. Eysenck's theory concerns the extraversion, neuroticism, and psychoticism traits, whereas Gray proposed the use of the rotated axes of impulsivity and anxiety. Eysenck used multiple arousal systems as central explanatory constructs, whereas Gray describes more specific systems related to behavioral inhibition and activation. The authors review evidence relating to these theories and discuss key predictive successes and failures and methodological problems. There is a core of support for the Eysenck theory in some paradigms, such as the moderator effect of stimulation level on individual differences in phasic electrodermal response and eyelid conditioning. In other settings, the theory fails to explain empirical data adequately, especially in studies of subjective response and attention and performance. Gray's theory has advanced research through stimulating interest in moderation of personality effects by motivational variables and provides a better explanation than Eysenck's theory for certain data, such as instrumental conditioning to reward stimuli and the positive affectivity of extraverts. Overall, however, Gray's theory explains a narrower ranges of findings than Eysenck's. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Matthews, Gerald and Gilliland, Kirby},
	Journal = {Personality and Individual Differences},
	Keywords = {H. J. Eysenck's vs J. A. Gray's theories of biological bases of personality traits & brain functioning in individual differences Personality Theory [3140]},
	Number = {4},
	Pages = {583-626},
	Title = {The personality theories of { H. J. Eysenck and J. A. Gray}: A comparative review},
	Volume = {26},
	Year = {1999}}

@article{matthews:uwist,
	Abstract = {Presents psychometric and validation evidence on the University of Wales Institute of Science and Technology (UWIST) Mood Adjective Checklist (UMACL). A factor analysis of 388 Ss' responses confirmed that the UMACL measures dimensions of energetic arousal, tense arousal, and hedonic tone. Psychometric properties of UMACL scales were satisfactory. Discriminant validity was established by showing correlations between UMACL scales and demographic and personality variables. Significant correlations between the arousal scales and psychophysiological measures of autonomic arousal demonstrate concurrent validity. UMACL scales are also sensitive to external stressors. Certain stressors appear to evoke a more general stress syndrome associated with reduced energetic arousal and hedonic tone and increased tense arousal. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Matthews, Gerald and Jones, Dylan M. and Chamberlain, A. Graham},
	Journal = {British Journal of Psychology},
	Keywords = {psychometrics & validity & factor analysis of Mood Adjective Checklist, assessment of energetic & tense arousal & hedonic tone, college students, Wales Personality Scales & Inventories [2223]; Personality Traits & Processes [3120]},
	Number = {1},
	Pages = {17-42},
	Title = {Refining the measurement of mood: The UWIST Mood Adjective Checklist},
	Volume = {81},
	Year = {1990}}

@article{matthews:98,
	Abstract = {Reviews the application of dimensional personality trait models to clinical theory and practice. Assessment of traits is useful for understanding the individual client, for diagnosis and therapy, and for tailoring the clinical interview to the needs of the patient. Dimensional models have been applied to several areas of pathology. Neuroticism and a variety of other traits are implicated in subclinical stress reactions. At the process level, the effects of traits such as neuroticism may be mediated by maladaptive coping strategies. Traits such as neuroticism may also have a causal effect on more severe mood and anxiety disorders, although it is likely that there are reciprocal linkages between personality and pathology. As with subclinical stress, traits may influence depression and anxiety through abnormality of cognitive processes, the clinical significance of which should be assessed in the light of the individual's trait characteristics. A further area of application is personality disorder: Dimensional models capture and clarify the principal clinical features of conditions such as schizoid and antisocial disorders. It is concluded that the dimensional approach provides a framework for the systematic understanding of clinical expressions of abnormality in personality and their implications for diagnosis and treatment. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Matthews, Gerald and Saklofske, Donald H. and Costa, Paul T., Jr. and Deary, Ian J. and Zeidner, Moshe},
	Journal = {European Journal of Psychological Assessment},
	Keywords = {dimensional models of personality & application to systematic clinical assessment of psychopathology Personality Traits & Processes [3120]},
	Number = {1},
	Pages = {36-49},
	Title = {Dimensional models of personality: A framework for systematic clinical assessment},
	Volume = {14},
	Year = {1998}}

@incollection{mccrae:99,
	Abstract = {States that the Five-Factor Model (FFM) of personality is an empirical generalization about the covariation of personality traits, and proposes that neither the model itself nor the body of research findings with which it is associated constitutes a theory of personality. The authors summarize and elaborate on the Five-Factor Theory (FFT; R. R. McCrae and P. T. Costa, 1996) of personality that represents an effort to construct such a theory that is consistent with current knowledge. FFT is a contemporary version of trait theory, based on the assumptions that people are knowable, rational, variable, and proactive. FFT explains personality functioning as the operation of a universal personality system, with defined categories of variables and classes of dynamic processes that indicate the main causal pathways. The five personality factors--Neuroticism, Extraversion, Openness, Agreeableness, and Conscientiousness--form the nucleus of the system. FFT provides a framework in which to understand the development and operation of psychological mechanisms and the behavior and experience of individuals. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Address = {New York, NY},
	Author = {McCrae, Robert R. and Costa, Paul T., Jr.},
	Booktitle = {Handbook of personality: Theory and research (2nd ed.)},
	Editor = {Pervin, Lawrence A. and John, Oliver P.},
	Keywords = {Five-Factor theory of personality Personality Psychology [3100]},
	Pages = {139-153},
	Publisher = {Guilford Press},
	Title = {A Five-Factor theory of personality},
	Year = {1999}}

