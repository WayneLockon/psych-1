@article{borsboom:02,
	Abstract = {Comments on the article by R. F. Krueger (see record 1999-01131-010) on the structure of common mental disorders. Krueger reported on the analysis of comorbidity data obtained in a sample of 8098 noninstitutionalized US civilians in the National Comorbidity Survey. The analysis was carried out using confirmatory analysis, which yields a 3-factor model that accounts reasonably well for the obtained covariance between several mental disorders. This led Krueger to conclude that "The results offer a novel perspective on comorbidity, suggesting that comorbidity results from common, underlying core psychopathological processes." The current author notes, however, that diagnoses were made using criteria of the Diagnostic and Statistical Manual of Mental Disorders-III-Revised (DSM-III-R) "without the imposition of hierarchical exclusionary rules." It is stated that this is fine as long as correlations among disorders are interpreted as reflecting comorbidity patterns, the problem arises when the obtained correlations are subjected to a confirmatory factor analysis and the resulting factors are interpreted as "underlying core psychopathological processes." It is concluded that Krueger's results cannot be regarded as evidence for such processes. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Borsboom, Denny},
	Journal = {Archives of General Psychiatry},
	Keywords = {confirmatory factor analysis, comorbidity, common mental disorders, psychopathological processes Psychological Disorders [3210]; Research Methods & Experimental Design [2260]},
	Number = {6},
	Pages = {569-570},
	Title = {The structure of the DSM},
	Volume = {59},
	Year = {2002}}

@book{borsboom:05,
	Abstract = {(from the cover) Is it possible to measure psychological attributes like intelligence, personality, and attitudes and if so, how does that work? What does the term 'measurement' mean in a psychological context? This book discusses these questions and investigates the possible answers that can be given in response. The author provides an in-depth treatment of the philosophical foundations of widely used measurement models in psychology. The theoretical status of classical test theory, latent variable theory, and representational measurement theory are critically evaluated, and positioned in terms of the underlying philosophy of science. Special attention is devoted to the central concept of test validity, and future directions to improve the theory and practice of psychological measurement are outlined. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Address = {viii, 185 pp. New York, NY},
	Author = {Borsboom, Denny},
	Keywords = {psychometrics, representational measurement theory, classical test theory, latent variable theory, test validity Psychometrics & Statistics \& Methodology [2200]},
	Publisher = {Cambridge University Press Cambridge University Press},
	Title = {Measuring the Mind: Conceptual Issues in Contemporary Psychometrics},
	Year = {2005}}

@article{http://dx.doi.org/10.1097/01.mlr.0000245143.08679.cc,
	Abstract = {The articles on differential item functioning (DIF) collected here represent a useful project in the application of bias detection methods. Each set of authors analyzes the Mini-Mental State Examination (MMSE) for measurement invariance using the same data, albeit with different methods. Together, the articles provide a sort of natural experiment on the robustness of biasing effects with respect to the choice of method. Although the present collection of articles on DIF detection show considerable agreement on the time and direction of biasing effects in the MMSE, some disagreement is also evident. This disagreement is likely due to the use of different empirical criteria for diagnosis of DIF. Evidently, whether bias is to be considered an important validity threat is not a straightforward function of p values or effect sizes. The reason for this is that the importance of bias is partly a pragmatic issue: It depends on aspects of the research situation that are not statistical in nature, such as the purposes for which the test scores are being used. Investigating measurement invariance should now become a routine part of research into the structure of group differences. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Borsboom, Denny},
	Journal = {Medical Care},
	Keywords = {measurement invariance, differential item functioning, Mini-Mental State Examination, experimental methods, group differences Research Methods & Experimental Design [2260]},
	Number = {11, Suppl 3},
	Pages = {S176-S181},
	Title = {When Does Measurement Invariance Matter?},
	Volume = {44},
	Year = {2006}}

@article{borsboom:02,
	Abstract = {Comments on the article by F. L. Schmidt and J. E. Hunter (1999), who argue that the correction for attenuation should be routinely used in theory testing. It is maintained that Schmidt and Hunter's arguments are based on mistaken assumptions. The current authors critique Schmidt and Hunter in terms of two arguments against a routine use of the correction for attenuation within the classical test theory framework: (1) corrected correlations do not, as Schmidt and Hunter claim, provide correlations between constructs, and (2) corrections for measurement error should be made using modem test theory models instead of the classical model. It is concluded that most measurement problems in psychology concern issues of validity, and that the correction for attenuation within classical test theory does not help in solving them. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Borsboom, Denny and Mellenbergh, Gideon J.},
	Journal = {Intelligence},
	Keywords = {true scores, latent variables, constructs, commentary, theory testing, correction for attenuation, classical test theory, modern test theory Statistics \& Mathematics [2240]},
	Number = {6},
	Pages = {505-514},
	Title = {True scores, latent variables and constructs: A comment on Schmidt and Hunter},
	Volume = {30},
	Year = {2002}}

@article{borsboom:04,
	Abstract = {This paper comments on an article by Michell (see record 2000-16131-004) who argues that psychometrics should be qualified as pathological science for two reasons: (a) psychometrics assumes psychological attributes to be quantitative without testing this hypothesis; and (b) the fact that this hypothesis is not tested is disguised. Michell further argues that the hypothesis should be tested using additive conjoint measurement theory. Although relevant to classical test theory, Michell's arguments do not apply to psychometrics in general. In particular, they are largely irrelevant to item response theory models. We show that these models result from introducing probabilistic relations, which are needed to deal with measurement error, and not from a breakdown in critical inquiry, as Michell suggests. Moreover, at least one class of these models can be formulated in terms of additive conjoint measurement theory, which renders Michell's call for the additive conjoint model in need of qualification. Finally, item response theory models are routinely tested against empirical data, and although the assumption that an attribute is quantitative cannot be tested directly, such tests do address the conjunction of this assumption and other model assumptions. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Borsboom, Denny and Mellenbergh, Gideon J.},
	Journal = {Theory \& Psychology},
	Keywords = {item response theory, empiricism, quantitative psychology, psychometrics, pathology of science Research Methods & Experimental Design [2260]},
	Number = {1},
	Pages = {105-120},
	Title = {Why psychometrics is not pathological},
	Volume = {14},
	Year = {2004}}

@article{http://dx.doi.org/10.1037/0033-295X.110.2.203,
	Abstract = {This article examines the theoretical status of latent variables as used in modern test theory models. First, it is argued that a consistent interpretation of such models requires a realist ontology for latent variables. Second, the relation between latent variables and their indicators is discussed. It is maintained that this relation can be interpreted as a causal one but that in measurement models for interindividual differences the relation does not apply to the level of the individual person. To substantiate intraindividual causal conclusions, one must explicitly represent individual level processes in the measurement model. Several research strategies that may be useful in this respect are discussed, and a typology of constructs is proposed on the basis of this analysis. The need to link individual processes to latent variable models for interindividual differences is emphasized. (PsycINFO Database Record (c) 2006 APA, all rights reserved) (journal abstract)},
	Author = {Borsboom, Denny and Mellenbergh, Gideon J. and van Heerden, Jaap},
	Journal = {Psychological Review},
	Keywords = {latent variables, interindividual differences, theoretical status, latent variable models, causality, realist ontology, individual processes, measurement model General Psychology [2100]},
	Number = {2},
	Pages = {203-219},
	Title = {The theoretical status of latent variables},
	Volume = {110},
	Year = {2003}}

@article{borsboom:valid,
	Abstract = {This article advances a simple conception of test validity: A test is valid for measuring an attribute if (a) the attribute exists and (b) variations in the attribute causally produce variation in the measurement outcomes. This conception is shown to diverge from current validity theory in several respects. In particular, the emphasis in the proposed conception is on ontology, reference, and causality, whereas current validity theory focuses on epistemology, meaning, and correlation. It is argued that the proposed conception is not only simpler but also theoretically superior to the position taken in the existing literature. Further, it has clear theoretical and practical implications for validation research. Most important, validation research must not be directed at the relation between the measured attribute and other attributes but at the processes that convey the effect of the measured attribute on the test scores. (PsycINFO Database Record (c) 2006 APA, all rights reserved) (journal abstract)},
	Author = {Borsboom, Denny and Mellenbergh, Gideon J. and van Heerden, Jaap},
	Journal = {Psychological Review},
	Keywords = {test validity, attribute measurement, validity theory, research Psychometrics & Statistics & Methodology [2200]},
	Number = {4},
	Pages = {1061-1071},
	Title = {The Concept of Validity},
	Volume = {111},
	Year = {2004}}

