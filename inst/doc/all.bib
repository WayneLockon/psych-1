@article{anderson:94,
	Abstract = {According to the Yerkes-Dodson law, performance is an inverted-U function of arousal with a negative relationship between optimal arousal and task difficulty. 100 college students differing in impulsivity completed easy (letter cancellation) and difficult (verbal abilities) tasks during the morning. Each S was tested with 5 different doses of caffeine. In any one session each S received either placebo or caffeine. Data were subjected to a traditional ANOVA and were analyzed individually. Group-level results indicate that performance was an interactive function of task, caffeine, and impulsivity. As predicted by the Yerkes-Dodson law, performance on the easy task tended to improve as caffeine dosage increased, but on the difficult task, (less aroused) impulsive Ss improved while (more aroused) nonimpulsive Ss improved then deteriorated. Both group and individual level analyses yielded results consistent with the Yerkes-Dodson law. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Anderson, Kristen Joan},
	Date-Added = {2008-03-03 10:42:42 -0600},
	Date-Modified = {2008-03-03 10:42:42 -0600},
	Journal = {Personality and Individual Differences},
	Keywords = {caffeine dose & arousal & impulsivity & task difficulty, performance consistent with Yerkes-Dodson Law, college students Psychopharmacology [2580]},
	Number = {6},
	Pages = {813-829},
	Title = {Impulsivity, caffeine, and task difficulty: A within-subjects test of the Yerkes-Dodson law},
	Volume = {16},
	Year = {1994}}

@article{anderson:90,
	Abstract = {After reviewing the literature linking threat, incentive, and relaxation to motor performance, R. Neiss (see record 1989-14248-001) concluded that both the construct of arousal and the hypothesis of an inverted-U relationship between performance and arousal should be abandoned. These arguments were, however, based on research that does not permit clear evaluation of either the construct of arousal or the inverted-U hypothesis. Furthermore, some of Neiss's assumptions are questionable. This article reviews evidence that provides strong support for the inverted-U hypothesis. It is concluded that the concept of arousal is pragmatically useful in organizing a broad range of behavioral data. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Anderson, Kristen J.},
