	Number = {3},
	Pages = {321--327},
	Title = {Determining the number of components from the matrix of partial correlations},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1007/BF02293557},
	Volume = {41},
	Year = {1976},
