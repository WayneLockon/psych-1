	Date-Modified = {2008-09-08 17:19:08 -0500},
	Journal = {Applied Psychological Measurement},
	Number = {2},
	Pages = {135-157},
	Title = {Estimating {$\omega_h$} for Structures Containing Two Group Factors: Perils and Prospects},
	Volume = {31},
	Year = {2007}}

