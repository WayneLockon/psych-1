@article{http://dx.doi.org/10.1177/0146621604265510,
	Abstract = {Mokken scale analysis (MSA) can be used to assess and build unidimensional scales from an item pool that is sensitive to multiple dimensions. These scales satisfy a set of scaling conditions, one of which follows from the model of monotone homogeneity. An important drawback of the MSA program is that the sequential item selection and scale construction procedure may not find the dominant underlying dimensionality of the responses to a set of items. The authors investigated alternative hierarchical item selection procedures and compared the performance of four hierarchical methods and the sequential clustering method in the MSA context. The results showed that hierarchical clustering methods can improve the search process of the dominant dimensionality of a data matrix. In particular, the complete linkage and scale linkage methods were promising in finding the dimensionality of the item response data from a set of items. (PsycINFO Database Record (c) 2006 APA, all rights reserved) (journal abstract)},
	Author = {van Abswoude, Alexandra A. H. and Vermunt, Jeroen K. and Hemker, Bas T. and van der Ark, L. Andries},
	Journal = {Applied Psychological Measurement},
	Keywords = {Mokken scale analysis, hierarchical clustering procedures, item pool, item selection methods, item response data Statistics & Mathematics [2240] Empirical Study; Quantitative Study},
	Number = {5},
	Pages = {332-354},
	Title = {{Mokken} scale analysis using hierarchical clustering procedures},
	Volume = {28},
	Year = {2004}}

@article{vanschuur:95,
	Abstract = {Analyzed data on the Bradburn Affect Balance Scale (BABS; N. M. Bradburn, 1969) collected from 10 European countries, to show that positive and negative affect items form a single cumulative scale and that the 2 cumulative scales taken together form 1 unidimensional unfolding scale. 12,463 Ss in 1981 and 15,385 in 1990 were interviewed and administered the BABS. The unfolding analyses was performed with a computer program (W. H. Van Schuur & W. Post, 1990). Results show that the BABS data conformed to the unidimensional unfolding scale, although there were few variations in the unfolding order of the items between 1981 and 1990. It is concluded that the BABS is an unfolding scale in which each of the 2 halves form a cumulative scale, and that it is a valid measure of perceived quality of life. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
