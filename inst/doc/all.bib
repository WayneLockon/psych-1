	Title = {A measurement model for likert responses that incorporates response time},
	Volume = {42},
	Year = {2007}}

@article{akert:panter:88,
	Abstract = {Examined the relationship between Extraversion, as measured by the Eysenck Personality Inventory, and skill at decoding nonverbal forms of communication in 34 college students. It was hypothesized that extraverts would decode nonverbal cues in social interaction more accurately than introverts. Data support this hypothesis. Extraverts were significantly more accurate in interpreting the meaning of nonverbal communication than introverts; extraverts were also more confident that they were accurate decoders than introverts. Results are discussed in terms of cognitive processing style: The extravert advantage in decoding nonverbal communication may be due to extraverts' superior attentive/perceptual skills; their superior interpretive/attributional skills; or both. Results are discussed in terms of methodological issues in the nonverbal decoding literature and their impact on research on extraversion. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Akert, Robin M. and Panter, Abigail T.},
	Journal = {Personality and Individual Differences},
	Keywords = {*Communication Skills *Extraversion *Nonverbal Communication *Personality Correlates Group & Interpersonal Processes [3020]. Human. Adulthood (18 yrs & older)},
	Number = {6},
