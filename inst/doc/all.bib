	Keywords = {personality-disorders hemispheric asymmetries individual-differences perception dimensions precedence model task},
	Number = {2},
	Pages = {123-129},
	Title = {Who sees trees before forest? {The } obsessive-compulsive style of visual attention},
	Volume = {16},
	Year = {2005}}

