@article{klirs,
	Abstract = {Proposes that the construing of situations by an individual as similar leads to consistency in responses to those situations and that, obversely, perceiving situations as dissimilar leads to inconsistency in responding. An interactional approach to the issue of transsituational consistency, which relates persons and situations via the mediating variable of a person's world view, is suggested. A model for the mediating variable is proposed that emphasizes the idiographic distortions of a shared nomothetic structure of the world. This model was tested using individual differences multidimensional scaling (INDSCAL) to recreate 100 individuals' judgments of similarity between 28 pairs of social situations. These INDSCAL-derived similarities (which conform to the model of the proposed mediating cognitive structure) significantly predicted variability across the situations for 58% of the Ss. The most predictable 50 Ss had an average correlation of .38 between the INDSCAL-recreated similarities and their self-reported transsituational variability. The least-predictable 50 Ss had an average correlation of only .09. The degree of fit of the INDSCAL model to the initial data was significantly related to how well a particular S's variability scores could be predicted from the INDSCAL model. The advantages of the proposed idiographic/nomothetic model over a purely idiographic model are discussed.},
	Author = {Klirs, Elisha G. and Revelle, William},
	Date-Added = {2008-04-01 18:42:14 -0500},
	Date-Modified = {2008-04-01 18:44:03 -0500},
	Journal = {Journal of Research in Personality},
	Keywords = {idiographic/nomothetic model of perceived situational similarity, self reported response variablility, college students Social Perception & Cognition},
	Number = {1},
	Pages = {34-50},
	Title = {Predicting variability from perceived situational similarity},
	Volume = {20},
	Year = {1986}}

