@article{dickens:flynn:02,
	Abstract = {In their original article (W. T. Dickens & J. R. Flynn, 2001), the authors formalized the consensus model of reciprocal effects between IQ and environment and showed that it can resolve the apparent paradox between high heritability and large environmental effects. Commentators suggested that the model has undesirable properties that call its usefulness into question. J. L. Loehlin (2002) argued that incorporating persistence of IQ into the model causes problematic behavior. D. C. Rowe and J. L. Rodgers (2002) argued that an increasing correlation of IQ and environment should have caused growing variance of IQ. Empirical evidence suggests that IQ is not sufficiently persistent to cause the problems Loehlin found and that the correlation of IQ and environment has not grown much over time so that the reciprocal effects model need not imply increasing variance. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (journal abstract)},
	Author = {Dickens, William T. and Flynn, James R.},
	Journal = {Psychological Review},
	Keywords = {model, genes, large environmental effects, IQ, generation gains, high heritability estimates Personality Traits & Processes [3120]},
	Number = {4},
