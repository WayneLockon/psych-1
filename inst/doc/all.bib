	Pages = {417-430},
	Title = {A nonparameteric approach to the analysis of dichotomous item responses},
	Volume = {6},
	Year = {1982}}

@article{mokken:86,
	Abstract = {Responds to comments by E. E. Roskam et al (see record 1987-09260-001) on the 1st author's (1971) nonparametric approach to constructing and evaluating tests based on binary items. It is contended that their arguments misrepresent the objectives of this approach, that their criticism of the role of the H coefficient in the procedures is irrelevant or erroneous, and that they fail to distinguish the inherent requirements (and limitations) of parametric and nonparametric models. It is concluded that the 1st author's procedures provide a useful tool for constructing and evaluating tests for measuring theoretically meaningful latent traits while avoiding the strong parametric assumptions of traditional item response theory. (21 ref) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Mokken, Robert J. and Lewis, Charles and Sijtsma, Klaas},
