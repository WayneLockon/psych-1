@article{horn:79,
	Abstract = {Demonstrates that R. B. Cattell's (1966) scree test and M. S. Bartlett's (see record 1951-00671-001) chi-square test for the number of factors are both based on the same rationale, so the former reflects statistical (S sampling) variability and the latter usually involves psychometric (variable sampling) influences. If the alpha-level (implicit in the scree test) is set the same, the 2 tests should lead to the same conclusions. Analyses with some examples suggest that if the alpha-level for the Bartlett test is set (explicitly) in the neighborhood of .0003 for sample Ns of 100-250, the results from applications of this test will indicate approximately the same number of factors as estimated on the basis of a scree test determined on approximately 600 Ss. Used in this way, the Bartlett test may yield fairly good "population" estimates of the number of factors. Relationships between the Bartlett test, hence the scree test, and tests for a common factor model and for the significance of a correlation matrix are explicated. (37 ref) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Horn, John L. and Engstrom, Robert},
	Journal = {Multivariate Behavioral Research},
	Keywords = {setting of alpha levels, comparison of R. B. Cattell's scree test & M. S. Bartlett's chi square test for factor analysis Statistics & Mathematics [2240]},
	Number = {3},
	Pages = {283-300},
	Title = {Cattell's scree test in relation to Bartlett's chi-square test and other observations on the number of factors problem},
	Volume = {14},
	Year = {1979}}

@article{howarth:68,
	Abstract = {Selected 110 female Ss with extreme extraversion scores. 55 extraverts and 55 introverts were allocated among 5 recall intervals: 0, 1, 5, 30 min., 24 hr. in a 10-cell design. The Ss were 1st trained to a criterion of once through correct on 7 CVC pairs, then were tested for recall of the material after the appropriate time interval. The results showed that extraverts were superior at the short-term intervals but inferior at the long-term intervals. These findings were interpreted in accord with Eysenck's theory that lower arousal in extraverts produces weaker consolidation processes which interfere less at short-term intervals but which do not facilitate long-term recall. (PsycINFO Database Copyright 1969 American Psychological Assn, all rights reserved)},
