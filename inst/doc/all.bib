	Title = {Cautionary Comments Regarding the Myers-Briggs Type Indicator},
	Volume = {57},
	Year = {2005}}

@article{litle,
	Abstract = {Administered a musical preference scale, devised from factor analysis of preference ratings for established categories of music based on divisions in the recording industry in the US, to 82 university students. The scales of the Sensation Seeking Scale--Form V were correlated with rated likings of each of the categories derived from the factor analysis. Total Sensation Seeking correlated positively with liking for all types of rock music and negatively with liking for bland soundtrack music. Results are consistent with the theory that high sensation seekers have a high optimal level of stimulation and thus tolerate and like high intensity and/or complexity in music, and stimulation in general. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Litle, Patrick and Zuckerman, Marvin},
