	Journal = {Psychological Bulletin},
	Keywords = {psychologically induced physiological arousal, motor performance & RT, commentary on literature review, application of inverted U hypothesis Motor Processes [2330]},
	Number = {1},
	Pages = {96-100},
	Title = {Arousal and the inverted-U hypothesis: A critique of {Neiss's} ``Reconceptualizing arousal"},
