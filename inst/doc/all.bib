@article{yung:thissen:99,
	Abstract = {The relationship between the higher-order factor model and the hierarchical factor model is explored formally. The authors show that the Schmid-Leiman transformation produces constrained hierarchical factor solutions. Using a generalized Schmid-Leiman transformation and its inverse, they show that for any unconstrained hierarchical factor model there is an equivalent higher-order factor model with direct effects (loadings) on the manifest variables from the higher-order factors. Therefore, the class of higher-order factor models (without direct effects of higher-order factors) is nested within the class of unconstrained hierarchical factor models. In light of these formal results, they discuss some implications for testing the higher-order factor model and the issue of general factor. An interesting aspect concerning the efficient fitting of the higher-order factor model with direct effects is noted. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Yung, Yiu-Fai and Thissen, David and McLeod, Lori D.},
	Date-Added = {2008-05-20 22:15:28 -0500},
	Date-Modified = {2008-05-20 22:15:41 -0500},
	Journal = {Psychometrika},
	Keywords = {Schmid-Leiman transformation in solution of higher-order factor model & hierarchical factor models Statistics & Mathematics [2240] Empirical Study},
	Number = {2},
	Pages = {113-128},
	Title = {On the relationship between the higher-order factor model and the hierarchical factor model},
	Volume = {64},
	Year = {1999}}

@article{montanelli:76,
	Abstract = {L. G. Humphreys and D. R. Ilgen's (see record 1970-17734-001) parallel analysis technique for determining the number of factors in factor analysis requires the construction of a 2nd correlation matrix, from normally distributed random numbers, using the same number of variables ( n ) and the same number of observations ( N ) as there are in the researcher's study. The eigenvalues from both matrices are plotted to determine the number of factors. However, predicting the size of the eigenvalues from the random matrices and determining their distribution is complicated and could entail expensive computer programming. To make the parallel analysis procedure easy to use, regression equations for predicting the logarithms of the latent roots of random correlation matrices, with squared multiple correlations on the diagonal, are presented. The correlation matrices were derived from distributions of normally distributed random numbers. The results of this method are excellent, with multiple correlation coefficients ranging from .9948 to .9992. When using these data, combined with observation of breaks in the curve of the latent roots, the investigator has a more objective and valid basis for deciding on the number of factors to rotate and interpret than previously has been available. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Montanelli, Richard G. and Humphreys, Lloyd G.},
	Date-Added = {2008-05-20 21:47:41 -0500},
	Date-Modified = {2008-05-20 21:48:02 -0500},
	Journal = {Psychometrika},
	Keywords = {latent roots of random data correlation matrices with squared multiple correlations on diagonal, Monte Carlo study Statistics & Mathematics [2240]},
	Number = {3},
	Pages = {341-348},
	Title = {Latent roots of random data correlation matrices with squared multiple correlations on the diagonal: A Monte Carlo study},
	Volume = {41},
	Year = {1976}}

@article{humphreys:75,
	Abstract = {When correlation matrices based upon real and random data with squared multiple correlations in the diagonals are factored, the hypothesis is that the point at which the curves of the latent roots cross indicates the number of common factors. Sampling studies are described which confirm the hypothesis when the common factor model provides a good fit to the data. When small overlapping, nonrandom factors are introduced, the expected value of the number of common factors can still be the number of major factors in the population when the nonrandom "noise" is small compared to sampling error "noise." This criterion for the number of common factors, furthermore, is more accurate than the method of maximum likelihood. (16 ref) (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Humphreys, Lloyd G. and Montanelli, Richard G.},
	Date-Added = {2008-05-20 21:47:41 -0500},
	Date-Modified = {2008-06-16 18:02:11 -0500},
	Journal = {Multivariate Behavioral Research},
	Keywords = {parallel analysis method, determining number of common factors Psychometrics & Statistics & Methodology [2200]},
	Number = {2},
	Pages = {193-205},
	Title = {An investigation of the parallel analysis criterion for determining the number of common factors},
	Volume = {10},
	Year = {1975}}

@article{http://dx.doi.org/10.1177/001316446902900202,
	Abstract = {9 correlation matrices were generated from distributions of random deviates, from which 18 separate factor matrices were obtained. To test hypotheses, target matrices were developed without reference to the unrotated loading; to form hypotheses, a subset of principal component factor matrices were rotated by means of the maxplane. Rotations were by both oblique and orthogonal methods. Number of Ss, number of variables, and number of factors contribute to the capitalization on chance effects in factor analytic studies. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
