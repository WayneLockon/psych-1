	Journal = {Psychological Science},
	Keywords = {happiness vs sadness & dimensional vs hierarchical bipolar models of positive vs negative affect, college students, commentary reply Personality Traits & Processes [3120]},
	Number = {4},
	Pages = {307-309},
	Title = {Further support for a hierarchical model of affect: Reply to Green and Salovey},
	Volume = {10},
	Year = {1999}}

@article{tellegen:99,
	Abstract = {Examined D. P. Green, S. L. Goldman, and P. Salovey's (1993) study that challenged the view that positive and negative affect are largely uncorrelated dimensions. Their study focuses on happiness and sadness and positive and negative emotional activation (PA and NA). The authors of this study claimed that a largely bipolar structure of affect emerges when random and nonrandom error are taken into account. This article also discusses D. Waston and A. Tellegen's (1985) model which is also focused on a bipolar model of affect, but arrives at a different conclusion. 284 college students participated in this study which deals with happiness and sadness, which forms a largely unidimensional bipolar structure, but PA and NA are relatively independent. However, exploratory analyses yield a 3-level hierarchy incorporating in 1 structure a general bipolar happiness vs unhappiness dimension, the relatively independent PA and NA dimensions at the level below it, and discrete emotions at the base. The heuristic value of a hierarchical perspective is emphasized. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Tellegen, Auke and Watson, David and Clark, Lee Anna},
	Journal = {Psychological Science},
	Keywords = {happiness vs sadness & dimensional vs hierarchical bipolar models of positive vs negative affect, college students Personality Traits & Processes [3120]},
	Number = {4},
	Pages = {297-303},
	Title = {On the dimensional and hierarchical structure of affect},
	Volume = {10},
	Year = {1999}}

@article{thayer:70,
	Abstract = {Examines the validity of controlled verbal reports of various activation states. A difference score design was used in which 4 psychophysiological measures and verbal ratings of various activation states were obtained in a base line and an activation period from 41 female undergraduates. Verbal reports were then correlated with individual physiological measures and composites or indices of physiological measures. Specifically, 2 kinds of physiological index were employed, 1 in which the S's physiological change score was represented by the single system showing the greatest change, and a 2nd index weighted equally by all 4 physiological measures. The physiological index using the single system showing the greatest activation yielded slightly greater correlations with verbal report than the other index. Skin conductance and heart rate, the best combination of the 4 physiological systems measured, correlated as high as .62 with verbal report. Results demonstrate the usefulness and validity of controlled self-report and the relative superiority of skin conductance and heart rate among other physiological systems in correlations with verbal report. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
