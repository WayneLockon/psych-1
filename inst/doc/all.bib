@article{eysenck:97,
	Abstract = {It is suggested that the scientific status of psychology is put in danger by the lack of paradigms in many of its fields, and by the failure to achieve unification, psychology is breaking up into many different disciplines. One important cause was suggested by Lee Cronbach in his 1957 presidential address to the American Psychological Association: the continuing failure of the two scientific disciplines of psychology--the experimental and the correlational--to come together and mutually support each other. Personality study in particular has suffered from this disunity, and the debates about the number of major dimensions of personality. Examples are given to show that by combining methods and theories typical of these two disciplines, one can be put forward paradigms that would be impossible without such unification. Such a paradigm is suggested for personality and intelligence. (PsycINFO Database Record (c) 2006 APA, all rights reserved) (journal abstract)},
	Author = {Eysenck, Hans J.},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {unifying experimental & correlational psychology methods, creation of paradigms for personality & intelligence Personality Psychology [3100]},
