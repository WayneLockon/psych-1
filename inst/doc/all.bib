@article{beck:97,
	Abstract = {A three-stage schema-based information processing model of anxiety is described that involves: (a) the initial registration of a threat stimulus; (b) the activation of a primal threat mode, and (c) the secondary activation of more elaborative and reflective modes of thinking. The defining elements of automatic and strategic processing are discussed with the cognitive bias in anxiety reconceptualized terms of a mixture of automatic and strategic processing characteristics depending on which stage of the information processing model is under consideration. The goal in the treatment of anxiety is to deactivate the more automatic primal threat mode and to strengthen more constructive reflective modes of thinking. Arguments are presented for the inclusion of verbal mediation as a necessary but not sufficient component in the cognitive and behavioral treatment of anxiety. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (journal abstract)},
	Author = {Beck, Aaron T. and Clark, David A.},
	Date-Added = {2008-04-15 15:02:10 -0500},
	Date-Modified = {2008-04-15 15:03:39 -0500},
	Journal = {Behaviour Research and Therapy},
	Keywords = {3 stage schema based information processing model of anxiety with automatic & strategic processing features, implications for treatment of anxiety Neuroses & Anxiety Disorders [3215]},
	Month = {Jan},
	Number = {1},
	Pages = {49-58},
	Title = {An information processing model of anxiety: Automatic and strategic processes},
	Volume = {35},
	Year = {1997}}

