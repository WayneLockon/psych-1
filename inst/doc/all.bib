@incollection{krueger:02,
	Abstract = {Following a discussion of the person-level, variable-level, and research-design concerns in applying the comorbidity concept to psychopathological conditions, the author presents research that attempts to understand "comorbidity" as a signal of meaningful patterns of correlation among putatively separate mental disorders. The idea is that specific mental disorders described in DSM may be conceived of as facets of broad, underlying dimensions of psychopathological variation. The goal of the research is to detect the signatures of these dimensions by examining patterns of correlations among DSM-derived diagnostic variables. Findings from two samples--the Dunedin Multidisciplinary Health and Development Study and the National Comorbidity Survey--are subjected to confirmatory factor analysis as applied to tetrachoric correlations among DSM-defined mental disorder diagnoses derived from structured interviews. Results of the analyses suggest that internalizing and externalizing are replicable phenotypic dimensions undergirding patterns of correlations among common mental disorders. The evidence suggests that a psychometric perspective is useful in making sense of comorbidity among common mental disorders. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Address = {Washington, DC},
	Author = {Krueger, Robert F.},
	Booktitle = {Defining psychopathology in the 21st century: DSM-V and beyond},
	Editor = {Helzer, John E. and Hudziak, James J.},
	Keywords = {comorbidity among mental disorders, psychopathological variation, confirmatory factor analysis, tetrachoric correlations, internalizing-externalizing poles Psychological & Physical Disorders [3200]},
	Pages = {41-54},
	Publisher = {American Psychiatric Publishing, Inc American Psychiatric Publishing, Inc},
	Title = {Psychometric perspectives on comorbidity},
	Year = {2002}}

@article{krueger:01,
	Abstract = {The authors hypothesized that anxiety and unipolar mood disorders are often comorbid because each disorder indicates a broad, higher order factor. In a clinical subsample of the nationally representative National Comorbidity Survey participants (N=251), a one-factor model fit the correlations among 7 dichotomous anxiety and unipolar mood diagnoses. Following the lead provided by literature on the structure of emotional and behavioral problems in children, we labeled this factor internalizing. Item response theory was used to explore how each diagnosis mapped onto the internalizing factor. The test information function derived from the 7 diagnoses suggested that they measure primarily the higher end of the factor. In addition, very high scores on internalizing (meeting criteria for 6-7 disorders) were associated with increased social costs, a phenomenon not well captured by the "comorbidity" concept. The results underscore the need to develop clinical assessment instruments that span the full range of the internalizing factor and measure both the shared and distinctive features of anxiety and unipolar mood disorders in a graded, continuous fashion. (PsycINFO Database Record (c) 2006 APA, all rights reserved) (journal abstract)},
	Author = {Krueger, Robert F. and Finger, Michael S.},
	Journal = {Psychological Assessment},
	Keywords = {using item response theory to understand comorbidity among 7 unipolar mood & anxiety disorders, 15-54 yr olds Psychological Disorders [3210]},
