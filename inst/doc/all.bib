@article{shigehisa:73,
	Abstract = {Determined auditory thresholds in 30 college students by a modified method of limits under 10 intensities of light (patterned or homogeneous). An analysis of variance shows significant results for intensity conditions, personality type (as measured by the Eysenck Personality Inventory-EPI), and the interaction of these parameters. Auditory sensitivity increased under weak intensities of light and decreased under strong intensities in introverts. It increased under all intensities in extraverts, with greater increase at greater intensities, and increased under weak and medium intensities in ambiverts. There was a positive correlation between sensitivity increases and scores on the EPI Extraversion (but not Neuroticism) scale at medium and strong intensities of light. (24 ref.) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Shigehisa, T. and Symons, J. R.},
	Journal = {British Journal of Psychology},
	Keywords = {visual stimulus intensity, auditory sensitivity, introverted vs extraverted college students Human Experimental Psychology [2300]; Personality Psychology [3100]},
	Number = {2},
	Pages = {205-213},
	Title = {Effect of intensity of visual stimulation on auditory sensitivity in relation to personality},
	Volume = {64},
	Year = {1973}}

@article{sinn:03,
	Abstract = {The paper offers a proof that expected utility maximisation with logarithmic utility is a dominant preference in the biological selection process in the sense that a population following any other preference for decision-making under risk will, with a probability that approaches certainty, disappear relative to the population following this preference as time goes to infinity. The result is contrasted with Weber's and Fechner's Psychophysical Law which implies logarithmic sensation functions for objective physical stimuli.},
	Author = {Sinn, H. W.},
	Journal = {Geneva Papers on Risk and Insurance Theory},
	Keywords = {risk evolution behavior},
	Number = {2},
	Pages = {87-100},
	Title = {Weber's Law and the biological evolution of risk preferences: The selective dominance of the logarithmic utility function, 2002 Geneva Risk Lecture},
	Volume = {28},
	Year = {2003}}

