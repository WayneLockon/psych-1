@article{watson:88,
	Abstract = {I examined correlates of Negative Affect (NA) and Positive Affect (PA) through both within- and between-subjects analyses. Eighty subjects completed a daily questionnaire for 6-8 weeks. Each day they rated (a) their mood, (b) the extent to which they suffered from various minor physical problems, (c) their level of stress, (d) the time they spent socializing, and (e) whether or not they had exercised. Subjects also completed several trait tests measuring their general affective level, frequency of health problems, and social tendencies. A between-subjects analysis showed the expected pattern: Level of physical complaints and perceived stress were correlated with individual differences in NA but not in PA, whereas social indicators and frequency of exercise were related only to PA. The within-subjects results generally exhibited a similar pattern: Social activity and exercise were more strongly related to PA, whereas perceived stress was highly related to NA. However, the most significant finding was that, contrary to prediction, health complaints were as strongly related to intraindividual fluctuations in PA as in NA. Possible interpretations of the observed correlates of NA and PA are discussed. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Watson, David},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {*Emotional States *Exercise *Health *Social Interaction *Stress Individual Differences Personality Traits & Processes [3120]. Human. Adulthood (18 yrs & older)},
	Number = {6},
	Pages = {1020-1030},
	Title = {Intraindividual and interindividual analyses of positive and negative affect: Their relation to health complaints, perceived stress, and daily activities},
	Volume = {54},
	Year = {1988}}

@book{watson:00,
	Abstract = {(from the jacket) This book describes how most of daily experience consists of mixed affective states reflecting complex combinations of the basic emotions. The book shows how this continuous "stream of affect" is regulated by 2 general biobehavioral systems associated with negative and positive mood states, respectively. The evolutionary origins of these 2 basic systems are discussed, as are their affective, cognitive, biological, and behavioral components. With attention to salient theoretical and measurement issues, chapters examine short-term fluctuation in mood that result from such factors as stress, social activity, exercise, and weather, as well as from endogenous cycles or rhythms. The author then establishes the stability of affective levels over time and across different life contexts. Exploring the underlying consistency, he relates affectivity to the traits comprising the 5-factor model of personality. Finally, these biologically based individual differences are shown to have profound implications for theories of happiness, as well as for mental and physical health. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Address = {New York, NY},
	Author = {Watson, David},
	Keywords = {*Daily Activities *Emotional States *Personality Personality Traits & Processes [3120]. Human},
	Publisher = {Guilford Press},
	Series = {Emotions and social behavior},
	Title = {Mood and temperament},
	Year = {2000}}

@article{watson:clark:92,
	Abstract = {Investigated relations between measures of self-rated affect and markers of Neuroticism, Extraversion, Openness to Experience, Agreeableness, and Conscientiousness in 4 samples (N=1,328). Replicating previous research (e.g., P. T. Costa and R. R. McCrae; see record 1981-10468-001), strong associations were found between Neuroticism and the negative affects, and between Extraversion and the positive affects. Conscientiousness had an independent relation with positive affect, but this effect was due wholly to Attentiveness. Only the Achievement facet of Conscientiousness correlated with the positive affects. Hostility was associated with (low) Agreeableness. While the results for Neuroticism and Extraversion clarify the temperamental basis of these higher order trait dimensions, those obtained for Agreeableness and Conscientiousness illustrate the importance of examining personality-affect relations at the lower order level. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Watson, David and Clark, Lee A.},
	Journal = {Journal of Personality},
	Keywords = {*Emotional States *Models *Personality Theory *Personality Traits Personality Theory [3140]. Human. Adulthood (18 yrs & older)},
	Number = {2},
	Pages = {441-476},
	Title = {On traits and temperament: General and specific factors of emotional experience and their relation to the five-factor model},
	Volume = {60},
	Year = {1992}}

@incollection{watson:clark:97,
	Abstract = {(from the chapter) explore the nature, components, and correlates of Extraversion / this concept is often viewed typologically, with "extraverts" and "introverts" defining discrete and self-limited categories /// early conceptions of extraversion (inconsistent and contradictory aspects of the trait, origin of the extraversion concept, J. P. Guilford's model, H. J. Eysenck's model, R. B. Cattell's model, summary and integration of the early models) / contemporary conceptions of extraversion (extraversion in relation to positive and negative affect, P. T. Costa and R. R. McCrae's model, A. Tellegen's view: positive emotionality, R. T. Hogan's bipartite scheme) / an integrative model of the construct (a schematic model, descriptions of the component traits) / exploring the core of extraversion / origins of the higher-order construct (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
