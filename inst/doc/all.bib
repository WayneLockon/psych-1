@article{matthews:86,
	Abstract = {Conducted 2 experiments with 107 male and 9 female 18-30 yr olds to provide data on the relationship between personality and creativity and to test assumptions made by arousal theory in explaining interactive effects of extraversion and arousal on performance. The effects of Sixteen Personality Factor Questionnaire (16PF) extraversion and trait anxiety, noise, and arousal (measured by questionnaire) on a creativity index were studied. Extraversion and anxiety interacted as predicted by arousal theory. However, extraversion was independent of arousal, and data suggest that there was no marked linear or curvilinear relationship between creativity test performance and arousal. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Matthews, Gerald},
	Journal = {Personality and Individual Differences},
	Keywords = {*Anxiety *Creativity *Extraversion *Noise Effects *Physiological Arousal Performance Personality Traits & Processes [3120]. Human. Adulthood (18 yrs & older)},
	Number = {6},
	Pages = {751-761},
	Title = {The interactive effects of extraversion and arousal on performance: Are creativity tests anomalous?},
	Volume = {7},
	Year = {1986}}

@article{matthews:87,
	Abstract = {Studied the interrelationships of various dimensions of extraversion, self-report arousal, and physiological arousal, using 40 male undergraduates. Ss also performed a verbal intelligence test. Results show that a composite measure of skin conductance and heart rate was significantly positively correlated with a self-report general arousal measure. Results show that Sixteen Personality Factor Questionnaire (16PF) Extraversion and 2 primary extraversion factors, F (impulsivity) and H (sociability), were significantly negatively correlated with physiological arousal and/or 1 dimension of self-report arousal. An interactive effect of time of day, F, and general arousal on performance was found. It is suggested that relationships between extraversion and arousal may be epiphenomenal to interactive effects of extraversion or impulsivity and arousal on performance. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Matthews, Gerald},
	Journal = {Personality and Individual Differences},
	Keywords = {*Extraversion *Performance *Physiological Arousal Impulsiveness Psychophysiology [2560]. Human. Adulthood (18 yrs & older)},
	Number = {1},
	Pages = {9-16},
	Title = {Personality and multidimensional arousal: A study of two dimensions of extraversion},
	Volume = {8},
	Year = {1987}}

