@article{anselme:07,
	Abstract = { Behaviour is usually assumed to depend on the reach of a critical intensity--termed reactivity threshold--by its motivation. This view represents a simple, predictive theoretical framework in ethology and animal psychology. However, it is here argued that only the influence of an isolated motivation on behaviour can be explained that way; that such a view fails to account for behaviour when several motivations are jointly activated. Upon analysis, the classical theory of behaviour (CTB) proves to be under-specified and thus leads to three conceptual problems that make it logically inconsistent for the study of multiple motivations. A revision of the CTB, called anticipatory dynamics model (ADM), is then developed in order to bring a theoretical solution to these conceptual problems. The ADM hypothesizes that an organism's motivational interactions are due to the limitation of the organism's attentional resources.},
	Author = {Anselme, Patrick},
	Date-Added = {2008-10-05 22:07:04 -0500},
	Date-Modified = {2008-10-05 22:07:17 -0500},
	Journal = {Behavioural Processes},
	Keywords = {Anticipation; Attention; Behaviour; Inhibition; Interference; Motivation},
	Number = {3},
	Pages = {259--275},
	Title = {Some conceptual problems with the classical theory of behaviour},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/B6T2J-4N5TN6P-4/2/c87896ff86b330766fcd7d5d5e1babfb},
	Volume = {75},
	Year = {2007},
