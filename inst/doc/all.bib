	Editor = {Robins, Richard W. and Fraley, R. Chris and Krueger, Robert F.},
	Keywords = {longitudinal data, personality research, data analysis, analytic techniques Statistics & Mathematics [2240]; Personality Psychology [3100]},
	Pages = {543-556},
	Publisher = {Guilford Press},
	Title = {The analysis of longitudinal data in personality research},
	Year = {2007}}

@article{goldberg:cattell,
	Abstract = {Originally published in Contemporary Psychology: APA Review of Books, 1968, Vol 13(12), 617-619. Reviews the book, Objective Personality & Motivation Tests: A Theoretical Introduction and Practical Compendium by Raymond B. Cattell, Frank W. Warburton, Fred L. Damarin, Jr., and Arthur B. Sweney (see record 1967-35007-000). This book--like most of Cattell's works--is simply impossible to review sensibly in 2,000 words, for the volume contains at least two "books," both of considerable importance to psychologists studying personality and motivation. One of the "books" is a compendium of 412 tests, with descriptions of the 2,366 test scores that Cattell and his associates have derived from those tests (1 to 50 scores from each test), factor loadings of each test score (when available and above an arbitrary criterion), the psychologist's--and the subject's--test title, the appropriate age range for the test, some estimate of testing time, brief directions for test administration and scoring, a short extract of the test materials themselves-and, most importantly, a paragraph or two about the theory leading up to the development of the test, the rationale for its construction, and the stages of design and modification of the final test. (PsycINFO Database Record (c) 2008 APA, all rights reserved)},
