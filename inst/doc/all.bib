	Keywords = {coefficient alpha, internal consistency, reliability coefficients, reliability, measurement Research Methods & Experimental Design [2260] Empirical Study},
	Number = {3},
	Pages = {343-355},
	Title = {Coefficient alpha and related internal consistency reliability coefficients},
	Volume = {5},
	Year = {2000}}

@article{callender:79,
	Abstract = {Assessed the degree of overestimation due to capitalization on sampling error and investigated methods of handling it. Data were collected from 380 individuals administered the Test of Chemical Comprehension. Results show that the MSPLIT estimator was a better estimator of reliability than the usual L-sub-3 or L-sub-2. It overcame the problem of capitalization on sampling variation even on small sample sizes. The absolute magnitude of the underestimation of reliability by L-sub-3 was quite large. The use of simulation studies to explore the sampling distribution of MSPLIT estimators relative to other reliability coefficients is discussed. (7 ref) (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Callender, John C. and Osburn, H. G.},
