	Title = {Thin Slices of Behavior as Cues of Personality and Intelligence},
	Volume = {86},
	Year = {2004}}

@article{borkenau:01,
	Abstract = {Previous behavior-genetic research on adult personality relied primarily on self-reports or peer reports that may be subject to contrast effects, resulting in biased estimates of genetic and environmental influences. In the German Observational Study of Adult Twins (GOSAT), personality traits of 168 monozygotic (MZ) and 132 dizygotic (DZ) twin pairs were rated on 35 adjective scales, largely markers of the Big 5. The ratings were provided by 120 judges who never met the twins but observed videotaped behaviors of 1 twin of each pair in 1 of 15 different settings. The aggregated video-based trait ratings were highly reliable, and substantial correlations were obtained between MZ as well as DZ twins. Model-fit analyses suggested about 40% genetic, 25% shared environmental, and 35% nonshared environmental influence. Extraversion was the only trait that seemed not to be influenced by shared environment. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (journal abstract)},
	Author = {Borkenau, Peter and Riemann, Rainer and Angleitner, Alois and Spinath, Frank M.},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {genetic & environmental influences on personality traits based on observed behavior, 18-70 yr old monozygotic & dizygotic twin pairs Personality Traits & Processes [3120]; Genetics [2510]},
	Number = {4},
	Pages = {655-668},
	Title = {Genetic and environmental influences on observed personality: Evidence from the German Observational Study of Adult Twins},
