@article{tellegenetal:88,
	Abstract = {We administered the Multidimensional Personality Questionnaire (MPQ) to 217 monozygotic and 114 dizygotic reared-together adult twin pairs and 44 monozygotic reared-apart adult twin pairs. A four-parameter biometric model (incorporating genetic, additive versus nonadditive, shared family-environment, and unshared environment components) and five reduced models were fitted through maximum-likelihood techniques to data obtained with the 11 primary MPQ scales and its 3 higher order scales. Solely environmental models did not fit any of the scales. Although the other reduced models, including the simple additive model, did fit many of the scales, only the full model provided a satisfactory fit for all scales. Heritabilities estimated by the full model ranged from .39 to .58. Consistent with previous reports, but contrary to widely held beliefs, the overall contribution of a common family-environment component was small and negligible for all but 2 of the 14 personality measures. Evidence of significant nonadditive genetic effects, possibly emergenic (epistatic) in nature, was obtained for 3 of the measures. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Tellegen, Auke and Lykken, David T. and Bouchard, Thomas J. and Wilcox, Kimerly J. and Segal, Nancy L. and Rich, Stephen},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {*Environment *Genetics *Heterozygotic Twins *Monozygotic Twins *Personality Traits Genetics [2510]. Human. Adulthood (18 yrs & older)},
	Number = {6},
	Pages = {1031-1039},
	Title = {Personality similarity in twins reared apart and together},
	Volume = {54},
	Year = {1988}}

@article{thayeretal:88,
	Abstract = {Studied the relationship between endogenous diurnal rhythms of energetic arousal and various psychological functions and theoretical issues relating to H. J. Eysenck's (1967) arousal theory of extraversion and neuroticism. Self-reported arousal measurements were taken from 18 adults every hour of the waking day for 6 days. Substantial relationships were observed between obtained diurnal cycles of energetic arousal and previous interview estimates by Ss of the best and worst times for social, mood, and intellectual functions. Eysenck's Neuroticism scores were found to predict overall tension ratings, and tense arousal showed an interaction between Extraversion and time of day. Modifications of Eysenck's arousal theory based on the 1st author's 2-dimensional arousal model are discussed. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Thayer, Robert E. and Takahashi, Paula J. and Pauli, Judy A.},
	Journal = {Personality and Individual Differences},
	Keywords = {*Emotional States *Extraversion *Human Biological Rhythms *Physiological Arousal Cognitive Processes Neuroticism Social Behavior Psychophysiology [2560]. Human. Adulthood (18 yrs & older)},
	Number = {1},
