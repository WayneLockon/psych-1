@article{http://dx.doi.org/10.1207/s15327752jpa6303_8,
	Abstract = {Attempted to provide a shorter (but equally valid) version of L. R. Goldberg's (see record 1992-25730-001) set of 100 adjective markers for the "Big-Five" factor structure found in phenotypic personality description. The performance of the 100 markers in 12 data sets of self- or peer-ratings by 489 college students was scrutinized, leading to the selection of an optimally robust subset of only 40 adjectives. This "Mini-Marker" subset demonstrated unusually impressive features for an abbreviated inventory, consisting of 5 scales that show (in comparison to the original scales) less use of difficult items, lower interscale correlations, and somewhat higher mean inter-item correlations. Alpha reliabilities are somewhat lower. A format for administering this briefer inventory is appended. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:52:01 -0500},
	Date-Modified = {2008-03-23 15:52:01 -0500},
	Journal = {Journal of Personality Assessment},
	Keywords = {validity of brief version of L. R. Goldberg's adjectives inventory, assessment of Big 5 factor structure, college students Personality Scales & Inventories [2223]; Personality Traits & Processes [3120]},
	Number = {3},
	Pages = {506-516},
	Title = {Mini-Markers: A brief version of Goldberg's unipolar Big-Five markers},
	Volume = {63},
	Year = {1994}}

@article{http://dx.doi.org/10.1037/0022-3514.66.1.141,
	Abstract = {Personality attributes typically combine descriptive and evaluative aspects, as do most personality constructs and the factors in the Big Five taxonomy. However, an auxiliary representation, in which the descriptive and evaluative aspects are clearly separated, has technological utility in some areas of research and assessment. D. Peabody (1968, 1985) and H. G. Gough (1987) independently developed structural models of this type, each with 3 dimensions. Studies used a multiple-method, multiple-replication approach, with data sets involving a large, representative set of familiar personality adjectives, to uncover a structure with 5 orthogonal dimensions, 4 descriptive and 1 highly evaluative, easily measurable with adjective scales. These dimensions demonstrated a complex relation to previous Peabody factors and to the Big Five, and they may be useful in integrating some venerable constructs into the structure of personality attributes. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:51:51 -0500},
	Date-Modified = {2008-03-23 15:51:51 -0500},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {descriptive vs evaluative aspects of personality attributes, college & graduate students Personality Traits & Processes [3120]},
	Number = {1},
	Pages = {141-154},
	Title = {Separating description and evaluation in the structure of personality attributes},
	Volume = {66},
	Year = {1994}}

@article{Saucier:1992lm,
	Abstract = {R. R. McCrae (see record 1991-01249-001) has argued that Openness to Experience is a fundamental dimension of personality that is not well represented among English-language trait adjectives. It is argued that the language does contain many adjectives referring to Openness and that recent studies of the language of personality reveal a large factor clearly related to both Openness and Intellect. These studies support both the Openness construct and the questionnaire version of the 5 basic factors. Lexical and questionnaire methods appear to converge on a single structural framework for basic personality traits. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:51:24 -0500},
	Date-Modified = {2008-03-23 15:51:27 -0500},
	Journal = {European Journal of Personality},
	Keywords = {need for natural language terms for openness personality trait, commentary Personality Traits & Processes [3120]},
	Number = {5},
	Pages = {381-386},
	Title = {Openness versus intellect: Much ado about nothing?},
	Volume = {6},
	Year = {1992}}

@article{http://dx.doi.org/10.1207/s15327752jpa7002_6,
	Abstract = {The NEO Personality Inventory--Revised (NEO-PI--R) has a short form, the NEO Five-Factor Inventory (NEO-FFI), that taps the 5 broad factors with fidelity and reliability. However, conventional scoring of this short form does not provide scores on more specific aspects of the broad-bandwidth factors. In this study, 13 item clusters were found to replicate across halves of a sample of self-descriptions by 732 adults. 13 factor-analytically derived scales were developed for the item clusters. The scales demonstrated reliability and factor structure comparable to that of the 30 facet scales of the NEO-PI--R. Correlation and multiple regression analyses showed that the content coverage of the 13 scales is more moderate. Archival and existing data involving the NEO-FFI can be easily rescored for these 13 subcomponents, increasing the researcher's gain of information from this convenient personality inventory. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:50:45 -0500},
	Date-Modified = {2008-03-23 15:50:45 -0500},
	Journal = {Journal of Personality Assessment},
	Keywords = {replicablity of item-cluster subcomponents of NEO Five Factor Inventory, adults Personality Traits & Processes [3120]; Personality Scales & Inventories [2223]},
	Number = {2},
	Pages = {263-276},
	Title = {Replicable item-cluster subcomponents in the NEO Five-Factor Inventory},
	Volume = {70},
	Year = {1998}}

@article{http://dx.doi.org/10.1111/1467-6494.694155,
	Abstract = {In judgments about personality, descriptive and evaluative aspects are ordinarily combined; separating them can be important both theoretically and practically. Study 1 showed that 2 similar descriptive factors can be found in analyses of personality terms, selected independently in 316 English Ss and in 362 German Ss and using different methods to control for evaluation. The factors relate to 2 pairs of independent axes suggested by previous work: Assertive-Unassertive and Tight-Loose, or alternatively, Interactional Orientation (Extraversion-Introversion) and Affective Orientation. These 2 pairs of axes are shown to be rotations of each other, and to form the prime non-evaluative circumplex. As in previous studies, non-evaluative scales elicited higher levels of self-peer agreement than did more typical evaluation-confounded scales. Study 2 showed that adjective scales for the octants of this circumplex have circular ordering, can fit even very stringent constraints of a circumplex model, have mild to strong isomorphism with the interpersonal circumplex, but represent somewhat broader constructs, and are systematically related to the Big 5 and the Big 3 personality factors. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard and Ostendorf, Fritz and Peabody, Dean},
	Date-Added = {2008-03-23 15:49:57 -0500},
	Date-Modified = {2008-03-23 15:49:57 -0500},
	Journal = {Journal of Personality},
	Keywords = {personality adjectives, circumplex Personality Traits & Processes [3120]},
	Number = {4},
	Pages = {537-582},
	Title = {The non-evaluative circumplex of personality adjectives},
	Volume = {69},
	Year = {2001}}

@article{http://dx.doi.org/10.1006/jrpe.2001.2335,
	Abstract = {The Big Five model, originally based on studies of lexical personality descriptors has proved useful and attracted much consensus. However, although the Big Five factors are held to be orthogonal, they are not orthogonal in previous marker set developed for them. The author describes methods for creating relatively orthogonal marker sets for orthogonal factors and demonstrates that the Big Five can be operationalized in relatively orthogonal marker scales. The three marker sets present are a 40-itern short form of Goldberg's 100 unipolar markers (Ortho-Markers), a new set of Modular Markers built with item parcels, and a 40-item short form of these (Mini-Modular Markers [the 3M40]). The new Big Five marker sets are shown to have markedly lower interscale correlations, with no loss of validity, relative to previous marker sets with comparable numbers of items. This indicates that the nonorthogonality of previous Big Five marker scales is less a property of the Big Five model than an unintended outcome of commonly used scale construction procedures. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:49:35 -0500},
	Date-Modified = {2008-03-23 15:49:35 -0500},
	Journal = {Journal of Research in Personality},
	Keywords = {Big Five model, personality, orthogonal, marker scales Personality Traits & Processes [3120]; Personality Scales & Inventories [2223]},
	Number = {1},
	Pages = {1-31},
	Title = {Orthogonal markers for orthogonal factors: The case of the Big Five},
	Volume = {36},
	Year = {2002}}

@article{http://dx.doi.org/10.1002/per.432,
	Abstract = {Comments on the article by M. C. Ashton and K. Lee (see record 2001-09727-001) which argues that Honesty should be added to the Big Five model of personality as a 6th factor. The current author applauds the attempt by Ashton and Lee to go beyond the Big Five, but highlights numerous problems with the concept. It is stated that adding Honesty to the Big Five is plausible only if one ignores key assumptions that the Big Five model consists of independent factors that are candidates for pervasive lexical universals. The proposal does not take into acount significant deviations from the Anglo-Germanic Big Five that have occurred in the emic studies of languages having their origin outside of northern Europe, nor potential substantive interpretations of the widely replicated Negative Valence factor. Recommendations for future studies are proposed. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:49:07 -0500},
	Date-Modified = {2008-03-23 15:49:07 -0500},
	Journal = {European Journal of Personality},
	Keywords = {lexical studies of personality structure, six-factor structure, traits related to prosocial/antisocial tendencies, traits that involve active engagement in domains of endeavor Personality Theory [3140]},
	Number = {1},
	Pages = {55-62},
	Title = {Gone too far--or not far enough? Comments on the article by Ashton and Lee (2001)},
	Volume = {16},
	Year = {2002}}

@incollection{Saucier:2002pw,
	Abstract = {(from the chapter) A factor is a parsimonious reduction of many observed variables into one hypothetical variable, accomplished within a particular set of data. The Big Five personality factor structure involves five orthogonal factors that capture the five largest sources of variance shared by the variables in fairly representative assemblages of personality-attribute descriptors in a number of languages. Whether the Big Five is the optimal cross-culturally generalizable taxonomic structure for human personality is still a matter of controversy, but it is clearly a very useful structure. This chapter describes various marker sets developed by the authors for the Big Five and related structures. We present these marker sets within a broader conceptual framework, reviewing 10 diverse psychometric criteria by which marker sets can be developed and evaluated. Because constructing a set of factor markers is typically an item-reduction exercise, we focus on the item selection process. The principles and issues we discuss are important to personality-test construction in general, in most cases applying also to scales that are not factor-analytically derived as are Big Five scales. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Address = {Ashland, OH},
	Author = {Saucier, Gerard and Goldberg, Lewis R.},
	Booktitle = {Big five assessment},
	Date-Added = {2008-03-23 15:48:33 -0500},
	Date-Modified = {2008-03-23 15:48:35 -0500},
	Editor = {de Raad, Boele and Perugini, Marco},
	Keywords = {psychometric criteria, item selection process, conceptual framework, taxonomic structure, related structures, personality factor, orthogonal factors, human personality, big five personality model Personality Scales & Inventories [2223]; Personality Traits & Processes [3120]},
	Pages = {30-54},
	Publisher = {Hogrefe & Huber Publishers},
	Title = {Assessing the big five: Applications of 10 psychometric criteria to the development of marker scales},
	Year = {2002}}

@incollection{Saucier:2006ya,
	Abstract = {(from the chapter) In this chapter we review progress on an important scientific issue-how attributes of personality and temperament can best be organized and structured. After explaining the rationale for doing so, we will discuss some insights that have been gained from studies of person descriptors in diverse languages. We discuss the degree to which one prominent structural model--the Big Five--is indeed an ideal model, and what other structural models are most complementary to this one in terms of strengths. Because inquiries into the structure of attributes depend significantly on how personality and temperament are defined, we begin there. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Address = {Mahwah, NJ},
	Author = {Saucier, Gerard and Simonds, Jennifer},
	Booktitle = {Handbook of personality development},
	Date-Added = {2008-03-23 15:47:53 -0500},
	Date-Modified = {2008-03-23 15:47:55 -0500},
	Editor = {Mroczek, Daniel K. and Little, Todd D.},
	Keywords = {personality structure, temperament structure Personality Traits & Processes [3120]},
	Pages = {109-128},
	Publisher = {Lawrence Erlbaum Associates Publishers},
	Title = {The Structure of Personality and Temperament},
	Year = {2006}}

@article{saucier:92,
	Abstract = {Previous research on the Big Five personality factors has not accounted for trait variables interstitial to the factor poles. To better integrate interstitial variables with the Big Five and to provide a framework for reconciling variant versions of the 5 factors, 636 self- and peer ratings using a set of 394 trait adjectives were analyzed. Pairings of 3 factors (I, II, and IV) showed a markedly large incidence of interstitial variables. These 3 factors, referencing affective and interpersonal traits, formed a 3-dimensional space. Adjective clusters defining both factor-univocal and interstitial benchmark positions in this space were developed. The 3 circles defined by the clusters showed appropriate circumplex characteristics when examined in an independent sample of 205 peer ratings. Two of these circles corresponded to the affective and interpersonal circles defined by personality research. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:47:38 -0500},
	Date-Modified = {2008-03-23 16:18:39 -0500},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {interstitial variables in affective & interpersonal trait factors of Big Five personality factor model, college students Personality Traits & Processes [3120]},
	Number = {6},
	Pages = {1025-1035},
	Title = {Benchmarks: Integrating affective and interpersonal circles with the Big-Five personality factors},
	Volume = {62},
	Year = {1992}}

@article{http://dx.doi.org/10.1037/0022-3514.76.4.613,
	Abstract = {An ideal structural representation of personality attributes would include more than just broad-bandwidth factors. Specific subcomponents help define broad factors while enhancing the fidelity of the representation. There has been no consensus with regard to the necessary specific subcomponents of the Big Five. This problem was addressed by analyzing 2 representative lexical data sets, one involving English adjectives and the other involving German adjectives. Large samples (Ns of 636 and 775) were used in classifying a selection of 500 adjectives in each language by Big Five domains, and within each domain and language, the terms were factor analyzed with promax rotation. Ratings by 22 bilinguals of correspondence between the adjectives in English and German factors indicated 18 distinct content themes common to personality description in the 2 languages. The 18 subcomponents delineate necessary features of a more finely faceted measurement model for the lexical Big Five factors. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (journal abstract)},
	Author = {Saucier, Gerard and Ostendorf, Fritz},
	Date-Added = {2008-03-23 15:47:12 -0500},
	Date-Modified = {2008-03-23 15:47:12 -0500},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {cross-language replication of specific subcomponents of Big Five personality factors, 15-81 yr olds Psychosocial & Personality Development [2840]},
	Number = {4},
	Pages = {613-627},
	Title = {Hierarchical subcomponents of the Big Five personality factors: A cross-language replication},
	Volume = {76},
	Year = {1999}}

@article{http://dx.doi.org/10.1037/0022-3514.78.2.366,
	Abstract = {Social attitude measurement has been limited by inadequate structural models. In this study, broad, basic dimensions were sought, with the assumption that crucial variables are represented as terms ending in -ism (isms). 266 isms were extracted from a dictionary, and items were built from their definitions. In a sample of 500 college students, the most replicable item structure had 3 factors; one of these 3 factors split into 2 factors in the 4-factor structure. A replication study confirmed that Conservatism and Authoritarianism are subcomponents of the largest factor. The other factors, though highly meaningful, seem more sparsely represented in previous research. No factor was highly related to personality traits other than Openness to Experience. The factors can serve as content-validity benchmarks for developing improved measurement models in this consequential, discrete domain of individual differences. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (journal abstract)},
	Author = {Saucier, Gerard},
	Date-Added = {2008-03-23 15:47:01 -0500},
	Date-Modified = {2008-03-23 15:47:01 -0500},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {methodological approach to uncovering major aspects of structure of individual differences in social attitudes, college students Personality Scales & Inventories [2223]; Social Perception & Cognition [3040]},
	Number = {2},
	Pages = {366-385},
	Title = {Isms and the structure of social attitudes},
	Volume = {78},
	Year = {2000}}

@article{jensen:weng,
	Abstract = {Examined the stability of psychometric g when g is extracted from a given correlation matrix by different models or methods of factor analysis. This was investigated both in simulated correlation matrices and in typical empirical data. While some methods are theoretically more appropriate for extracting g, g is actually remarkably robust across different methods of analysis, both in agreement between the estimated g and the true g in simulated data and in similarity among the g factors extracted from empirical data by different methods. Although the near-uniformity of g obtained by different methods suggests little basis for choosing or rejecting a particular method, certain factor models qua models may accord better than others with theoretical considerations about the nature of g. A reasonable strategy for estimating g, given an appropriate correlation matrix, is proposed. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Jensen, Arthur R. and Weng, Li-Jen},
	Date-Added = {2008-03-23 15:42:13 -0500},
	Date-Modified = {2008-03-23 15:45:05 -0500},
	Journal = {Intelligence},
	Number = {3},
	Pages = {231-258},
	Title = {What is a good g?},
	Volume = {18},
	Year = {1994}}

@article{gurtman:93,
	Abstract = {Shows how a structural model, the interpersonal circumplex, could be used to ensure that test items for measures of interpersonal problems (e.g., loneliness, nonassertiveness) faithfully mirror the theoretical characteristics of their constructs. New item statistics are derived with applicability to item analysis and test construction, and the equivalence of item-curve fitting and circumplex scoring is illuminated. This model was tested in a study with 279 college students who completed the UCLA Loneliness Scale, the Rathus Assertiveness Scale, and the Inventory of Interpersonal Problems. Results indicate that the interpersonal circumplex is more than a descriptive model and that it can play a prescriptive role in a rational-theoretical approach to test construction. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Author = {Gurtman, Michael B.},
	Date-Added = {2008-03-23 15:39:48 -0500},
	Date-Modified = {2008-06-01 12:01:57 -0500},
	Journal = {Journal of Personality},
	Keywords = {interpersonal circumplex structural model for selection & evaluation of test items of interpersonal constructs, college students tested for loneliness & nonassertiveness Personality Scales & Inventories [2223]; Personality Traits & Processes [3120]},
	Number = {2},
	Pages = {237-263},
	Title = {Constructing personality tests to meet a structural criterion: Application of the interpersonal circumplex},
	Volume = {61},
	Year = {1993}}

@incollection{gurtman:97,
	Abstract = {(from the chapter) show how circumplex, or circular models of personality could serve the broad purpose of construct elucidation / define the circumplex and relate this model to its principal alternative, the Big Five or Five-Factor Model / show how the key descriptive features of a trait--its thematic quality, breadth, and factorial saturation--can be obtained from the fundamentally geometric methods of analysis that derive from the circumplex / apply the methods to published data from a number of studies, demonstrating how these analytic tools can illuminate and bring context to the particular traits of interest /// if the interpersonal and affective domains conform generally to the circumplex model, then it should be possible to extract the circumplex factor space from any comprehensive measure of personality, and hence do similar, opportunistic analyses from suitably reconfigured measures / explore this possibility with the California Q-Set (CQ; J. Block, 1967, 1978) / suggest that other comprehensive personality measures would be amenable to similar analysis (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	Address = {Washington, DC},
	Author = {Gurtman, Michael B.},
	Booktitle = {Circumplex models of personality and emotions},
	Date-Added = {2008-03-23 15:39:29 -0500},
	Date-Modified = {2008-06-01 12:02:17 -0500},
	Editor = {Plutchik, Robert and Conte, Hope R.},
	Keywords = {similarity of circumplex & Five-Factor personality model & use of geometric analysis methods for trait description & with California Q-Set & other personality measures Personality Traits & Processes [3120]},
	Pages = {81-102},
	Publisher = {American Psychological Association},
	Title = {Studying personality traits: The circular way},
	Year = {1997}}

@incollection{gurtman:03,
	Abstract = {(from the preface) Notes that the appeal of circular models lies in the combination of a circle's aesthetic (organizational) simplicity and its powerful potential to describe data in uniquely compelling substantive and geometric ways, as has been demonstrated in describing interpersonal behavior and occupational interests. The present chapter discusses the application of the circumplex model to the description of individuals, comparisons of groups, and evaluations of constructs and their measures. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
