@article{swickert:gilliland:98,
	Abstract = {It has been suggested that the trait of extraversion is associated with physiological arousal. Both psychophysiological and behavioral studies have generally supported this statement. However, recent research has suggested that impulsivity is the trait most closely linked to the physiological arousal mechanisms believed to underlie extraversion. This study utilized brainstem auditory evoked responses (ABRs) to investigate which trait, of extraversion, impulsivity, and sociability, is most closely related to hypothesized ascending reticular activating system-regulated arousal processes in 74 participants. The results indicated that the full scale of extraversion was related to ABR wave V activity, with extraverts exhibiting slower ABR latencies compared to introverts. Analyses of the subscales indicated that sociability, rather than impulsivity, contributed more to this effect. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Swickert, Rhonda J. and Gilliland, Kirby},
	Journal = {Journal of Research in Personality},
	Keywords = {*Auditory Evoked Potentials *Extraversion *Impulsiveness *Introversion *Sociability Personality Traits & Processes [3120]; Electrophysiology [2530]. Human. Male. Female. Adulthood (18 yrs & older)},
	Number = {3},
	Pages = {314-330},
	Title = {Relationship between the brainstem auditory evoked response and extraversion, impulsivity, and sociability},
	Volume = {32},
	Year = {1998}}

@article{tamiretal:02,
	Abstract = {One must consider both trait and state affect to predict individual differences in emotional processing. The present results document a novel trait-state interaction that is consistent with proposals concerning the epistemic functions of affect (A. R. Damasio, 1994). Four studies tested the effects of extraversion and mood on motivation-relevant processing. Study 1 measured naturally occurring mood, whereas Studies 2-4 manipulated mood. Extraverts were faster to link events to their personal motivations when in a positive mood state, whereas introverts were fast to do so in a neutral or negative mood state. Further findings indicate that this interaction affects attitude accessibility rather than event elaboration. Overall, the authors suggest that there are pragmatic benefits to trait-consistent moods, particularly for processing motivation-relevant stimuli. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (journal abstract).},
	Author = {Tamir, Maya and Robinson, Michael D. and Clore, Gerald L.},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {*Cognitive Processes *Emotional States *Extraversion *Introversion Emotional Responses Epistemology Individual Differences Personality Traits & Processes [3120]. Human. Adulthood (18 yrs & older) us.},
	Number = {3},
