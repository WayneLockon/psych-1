	Number = {3},
	Pages = {255-263},
	Title = {Galen and the humour theory of temperament},
	Volume = {12},
	Year = {1991}}

@article{stemmler:97,
	Abstract = {Examined the traits as disposition concept by delineating boundary conditions for person and situation properties of aggressivity, using an anger induction procedure. 61 female Ss (aged 19-33 yrs) were studied under rest, handgrip, and vigilance tasks and then under 1 of 3 appraisal instructions (Full, Excuse, and Ignore provocation) intended to modify the intensity of anger provocation. Emotional self-reports indicated more anger in the Full than in the other provocation groups. Ratings of facial expressions and results of the post-experimental interview corroborated and extended these findings. Person-based boundary conditions were restricted since aggressivity, but neither neuroticism, achievement motivation extraversion, nor social desirability influenced the anger inductionxself-reported anger relationship. Across provocation groups aggressivity was selectively engaged only for above average aggressivity scores. Selective trait activation as an instance of personxenvironment interaction was explained with perceived situational demands and mutually inhibitory angerxfear relationships. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Stemmler, Gerhard},
	Journal = {Personality and Individual Differences},
	Keywords = {*Anger *Emotional Responses *Human Females *Personality Traits Personality Traits & Processes [3120]. Human. Female. Adulthood (18 yrs & older). Young Adulthood (18-29 yrs). Thirties (30-39 yrs)},
	Number = {2},
	Pages = {213-233},
	Title = {Selective activation of traits: Boundary conditions for the activation of anger},
	Volume = {22},
	Year = {1997}}

@article{strelau:87,
	Abstract = {Presents a historical perspective on the relationship between temperament and emotion and shows the place of emotion in current research on temperament. Emotion is considered as a trait mostly identified as emotionality (6 conceptions of which have been proposed), as a specific affective-motivational process, as a hedonic tone determined by specific limbic structures, and as a behavioral characteristic described in terms of intensity and temporal parameters. Some theories of temperament concentrate only on one of these roles, whereas in others several roles of emotion are exposed. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Strelau, Jan},
	Journal = {Journal of Research in Personality},
	Keywords = {*Emotional States *Personality Theory Personality Theory [3140]. Human},
