@article{goldberg:68,
	Abstract = {G. Lindzey (See 39:4) has previously presented findings purportedly relevant to the clinical vs. statistical prediction issue, and D. Meehl (See 39:4) has praised Lindzey's research as having provided "the 1st good example" of seer over sign. Meehl's conclusions are here questioned on the grounds that (1) Lindzey's data do not support Meehl's assertion that any seer demonstrated superior "validity generalization" over any sign, (2) the validity of the seers and signs did not differ at a statistically significant level in either of Lindzey's 2 studies, and, therefore, (3) these studies cannot be cited as "the 1st good example" of anything at all. The implications of this argument for future studies based upon the "validity generalization" paradigm is discussed. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Goldberg, Lewis R.},
	Journal = {Journal of Experimental Research in Personality},
	Keywords = {psychodiagnosis, clinical judgement vs. statistical prediction, validity generalization paradigm Psychological & Physical Disorders [3200]; Health & Mental Health Treatment & Prevention [3300]},
	Number = {2},
	Pages = {168-171},
	Title = {Methodological critiques. Seer over sign: The first``good" example?},
	Volume = {3},
	Year = {1968}}

@article{http://dx.doi.org/10.1037/h0029230,
	Abstract = {Clinical psychologists, physicians, and other professionals are typically called upon to combine cues to arrive at some diagnostic or prognostic decision. Mathematical representations of such clinical judges can often be constructed to capture critical aspects of their judgmental strategies. An analysis of the characteristics of such models permits a specification of the conditions under which the model itself will be a more valid predictor than will the man from whom it was derived. To ascertain whether such conditions are met in natural clinical decision making, data were reanalyzed from P. E. Meehl's (see 34:3) study of the judgments of 29 clinical psychologists attempting to differentiate psychotic from neurotic patients on the basis of their MMPI profiles. Results of these analyses indicate that for this diagnostic task models of the men are generally more valid than the men themselves. Moreover, the finding occurred even when the models were constructed on a small set of cases, and then man and model competed on a completely new set. (29 ref.) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Goldberg, Lewis R.},
	Journal = {Psychological Bulletin},
	Keywords = {discrimination of psychotic vs. neurotic patients, MMPI, clinicians' judgment vs. diagnostic model Health & Mental Health Treatment & Prevention [3300]},
	Number = {6},
	Pages = {422-432},
	Title = {Man versus model of man: A rationale, plus some evidence, for a method of improving on clinical inferences},
	Volume = {73},
	Year = {1970}}

@article{goldberg:mbr:72,
	Abstract = {Investigated the relative effects of 3 major sources of variance in multivariate predictions from personality inventories: (a) strategy of scale construction, (b) number of predictors included in the prediction functions, and (c) type of prediction functions utilized. 5 strategies of scale construction were used to construct 9 different 11-scale inventories from the CPI item pool, 5 based on 3 major strategies (external, internal, and intuitive) and 4 based on 2 control strategies (stylistic and random). Average cross-validities of each inventory were compared across 13 criterion indices, and across the 7 most predictable criteria, for each of 14 types of prediction functions and for differing numbers of predictors included in each function. Results were compared with orthogonal components and factors nonlinear and configural prediction models, other sets of CPI scales, and empirical item selection tactics. These analyses suggest that while the inventories constructed by the 3 major strategies produced quite similar average cross-validities, there was a sizable Criteria * Strategies interaction effect. Specifically, the external strategy appeared to produce a broader band-width but lower fidelity inventory than did the internal or intuitive strategies. Results were not limited to analyses using any particular number of scales in the prediction functions, nor to analyses based upon the traditional multiple-regression paradigm. However, a subset of 5 rational scales provided average cross-validities at least as high as those produced by the other strategies and tactics under study. (80 ref.) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Goldberg, Lewis R.},
	Journal = {Multivariate Behavioral Research Monographs. No 72-2 1972, 59. Society of Multivariate Experimental Psychology. Society of Multivariate Experimental Psychology},
	Keywords = {strategy of scale construction & number & type of prediction functions utilized for multivariate predictions, personality inventory construction & utilization Psychometrics & Statistics & Methodology [2200]},
	Title = {Parameters of personality inventory construction and utilization: A comparison of prediction strategies and tactics},
	Year = {1972}}

@article{http://dx.doi.org/10.1037/h0032723,
	Abstract = {Investigated the comparative advantages of data from group as compared to individual profiles. 233 group MMPI profiles were used to construct both sequential and simultaneous classification procedures for psychiatric diagnosis. Virtually perfect separation of the group profiles was accomplished by both procedures, the results ranging from 93% correct (psychotic vs. neurotic) to 99% correct (psychiatric vs. sociopathic). Moreover, a predictor index initially developed on individual profiles produced a cross-validity coefficient of .83 when applied to the group profiles. Findings suggest that group data appear to contain such a high signal to noise ratio that they become extraordinarily efficient indicators of underlying processes processes which are normally obscured by the unreliability inherent in individual profiles. Moreover, since these processes appear to be unusually well captured by a general linear model, the rather jaundiced views towards group profiles held by most of the clinical and psychometric community may well be unfounded. (20 ref.) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Goldberg, Lewis R.},
	Journal = {Journal of Abnormal Psychology},
	Keywords = {diagnostic classification systems construction, individual vs. group profiles Health & Mental Health Treatment & Prevention [3300]},
	Number = {2},
	Pages = {121-131},
	Title = {Man versus mean: The exploitation of group profiles for the construction of diagnostic classification systems},
