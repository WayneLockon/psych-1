@article{hofstee:97,
	Abstract = {Compared Big-Five factor structures found in Dutch, American English, and German, and presented a joint structure. The data consist of self- and peer ratings of 600 Ss with 551 Dutch trait-descriptive adjectives, 636 Ss with 540 English adjectives, and 802 Ss with 430 German adjectives. On the basis of 126 common items, the authors assessed the congruences between the factors as originally published, as resulting from target rotations, and from simultaneous rotations. With the exception of the Dutch Factor V, the Big-Five factors recur across languages in a relative but not in a strict sense. Moreover, at a more detailed level differences in the positions of the axes are uncovered. By applying a split-sample technique to the 3 data sets, the authors verify that these differences do not arise through unreliability. Also, few trait terms appear to have the same precise meaning across these 3 languages; such labels therefore cannot serve as anchor concepts for an international language of personality. (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Hofstee, Willem K. B. and Kiers, Henk A. and de Raad, Boele and Goldberg, Lewis R. and et al.},
	Journal = {European Journal of Personality},
	Keywords = {comparison of Big-Five Structures of personality traits in Dutch vs English vs German, German vs Dutch vs US college students Personality Traits & Processes [3120]},
