	Journal = {Quality of Life Research},
	M3 = {10.1007/s11136-007-9183-7},
	Month = {08},
	Number = {0},
	Pages = {19--31},
	Title = {The role of the bifactor model in resolving dimensionality issues in health outcomes measures},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1007/s11136-007-9183-7},
	Volume = {16},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s11136-007-9183-7}}

