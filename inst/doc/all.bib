@article{argyle:lu:90a,
	Abstract = {Predicted that happiness correlates strongly with extraversion and that this is due to the greater participation of extraverts in social activities. 130 Ss (aged 20-21 yrs) completed the Oxford Happiness Inventory, the E-Scale of the Eysenck Personality Questionnaire, and a 37-item scale for enjoyment of and participation in social and other activities. Happiness was correlated with extraversion, and extraverts enjoyed and participated more in social activities. Multiple regression showed that about half of the greater happiness of extraverts could be explained by their greater participation in social activities. (PsycINFO Database Record (c) 2007 APA, all rights reserved).},
	Author = {Argyle, Michael and Lu, Luo},
