	Keywords = {equations for computing confidence interval estimates and conducting F tests for intraclass correlation coefficients defined using 3-way random effects models for crossed & nested designs Statistics & Mathematics [2240]},
	Number = {2},
	Pages = {270-288},
	Title = {Confidence intervals and F tests for intraclass correlations based on three-way random effects models},
