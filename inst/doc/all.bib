	Journal = {Journal of Family Psychology},
	Keywords = {family psychology research, cluster analysis, clustering methods, hierarchical clustering, nonhierarchical clustering, cluster solutions Statistics & Mathematics [2240]; Marriage & Family [2950]},
	Number = {1},
