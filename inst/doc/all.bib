	Journal = {Methods of Psychological Research},
	Keywords = {circumplex structure, psychometric criteria, simulation, factor analysis, deviation scoring, variables interrelations, tests, simulations Research Methods & Experimental Design [2260] Empirical Study; Quantitative Study},
	Number = {1},
