@article{mokken:82,
	Abstract = {Examines an item response theory that is based on purely ordinal assumptions about the probabilities that individuals respond positively to items. It is considered to be a natural generalization of both Guttman scaling and classical test theory. A distinction is drawn between construction and evaluation of a test on the one hand and the use of a test to measure and make decisions about individuals' abilities on the other. Techniques to deal with each of these aspects are illustrated. (24 ref) (PsycINFO Database Record (c) 2006 APA, all rights reserved)},
	Author = {Mokken, Robert J. and Lewis, Charles},
	Journal = {Applied Psychological Measurement},
	Keywords = {item response theoretical approach based on purely ordinal assumptions about positive response probabilities, nonparametric analysis of dichotomous items Psychometrics & Statistics & Methodology [2200]},
