	Journal = {Journal of Personality Assessment},
	Keywords = {personality variables, interpersonal personality measures, interpersonal circle, interpersonal domain, interpersonal theory, psychometrics, circumplex structure, taxonomies Personality Scales & Inventories [2223]; Personality Traits & Processes [3120]},
	Number = {3},
